@using scaffold.Models

<h1>Wall</h1>
<h3>Post a Message!</h3>
<div>
    @Html.Partial("AddMessage", (MessageViewModel)@ViewBag.MesssageForm)
</div>
@{
    foreach (var message in ViewBag.Messages)
    {
        CommentViewModel Form = ViewBag.CommentForm;
        Form.message_id = message.id;
        <p>@Form.message_id</p>
        <br>
        <p>"@message.body"</p>
        <p class="text-primary">-@message.user.first_name @message.updated_date</p>
        @Html.Partial("AddComment", Form)
        <hr>
    }
}